{
  "manifest_version": 2,
  "name": "Clean Twitter",
  "description": "Remove all the annoying clutter from your Twitter.",
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["jquery.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab"]
}
