{
  "manifest_version": 3,
  "name": "Better Clean Twitter",
  "description": "Remove all the annoying clutter from your Twitter.",
  "version": "1.3.1",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "64": "icon.png",
    "128": "icon.png"
  },
  "key": "",
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "js/jquery.js",
        "js/content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_icon": "icon.png",
    "default_title": "Better Clean Twitter",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "scripting",
    "webRequest",
    "nativeMessaging"
  ],
  "host_permissions": [
    "*://twitter.com/*"
  ]
}